{
  "name": "@vercel/remix",
  "version": "2.7.0",
  "description": "Isomorphic runtime + adapter for Remix on Vercel",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/remix",
    "directory": "packages/vercel-remix"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "browser": "dist/edge/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/edge/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "browser": "./dist/edge/server.js",
      "default": "./server.js"
    },
    "./vite": {
      "types": "./vite.d.ts",
      "default": "./vite.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@remix-run/node": "2.7.0",
    "@remix-run/server-runtime": "2.7.0",
    "@vercel/static-config": "3.0.0",
    "isbot": "^3.6.8",
    "ts-morph": "12.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.7.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "defaults/",
    "globals.js",
    "globals.d.ts",
    "server.js",
    "server.d.ts",
    "vite.js",
    "vite.d.ts",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ]
}
